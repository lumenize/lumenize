{
  "name": "@lumenize/routing",
  "version": "0.21.0",
  "description": "Request routing for Cloudflare Durable Objects projects",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest --run",
    "coverage": "vitest --run --coverage",
    "test:watch": "vitest",
    "test:unit": "vitest --run --project unit",
    "test:integration": "vitest --run --project integration",
    "types": "wrangler types --config test/integration/wrangler.jsonc test/integration/worker-configuration.d.ts"
  },
  "dependencies": {
    "@lumenize/debug": "^0.21.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "@cloudflare/workers-types": "^4.20250110.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.38.0"
  },
  "peerDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "routing",
    "durable-objects",
    "cloudflare-workers"
  ],
  "author": "Larry Maccherone",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "9d965160b03757da225eca397b17ebf0161ad4d6"
}
