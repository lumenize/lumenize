{
  "name": "@lumenize/proxy-fetch",
  "version": "0.13.0",
  "description": "De✨light✨ful proxy fetch for cost-effective external API calls from Durable Objects",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "dev": "wrangler dev --show-interactive-dev-session=false",
    "test": "vitest --run",
    "test:live": "vitest --run --config vitest.live.config.js",
    "test:coverage": "vitest --coverage --run",
    "coverage": "COVERAGE=true vitest --run --coverage",
    "types": "cd test/do && wrangler types"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "durable-objects",
    "fetch",
    "queue",
    "cost-optimization",
    "dx"
  ],
  "author": "Lumenize",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "@vitest/coverage-istanbul": "3.2.4",
    "typescript": "^5.9.2",
    "vitest": "3.2.4",
    "wrangler": "^4.38.0"
  },
  "dependencies": {
    "@lumenize/utils": "^0.13.0",
    "ulid-workers": "^2.1.0"
  },
  "peerDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "vitest": "3.2.4"
  }
}
