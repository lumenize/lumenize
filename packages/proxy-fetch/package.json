{
  "name": "@lumenize/proxy-fetch",
  "version": "0.13.0",
  "description": "De✨light✨ful proxy fetch for cost-effective external API calls from Durable Objects",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "test": "npm run test:do && npm run test:queue",
    "test:do": "vitest --run",
    "test:queue": "vitest --run --config vitest.queue.config.js",
    "coverage": "npm run coverage:do; npm run coverage:queue; exit 0",
    "coverage:do": "vitest --run --coverage",
    "coverage:queue": "vitest --run --coverage --config vitest.queue.config.js",
    "test:local-live": "node test/production/local-live.test.mjs",
    "test:production": "node test/production/production.test.mjs",
    "dev:production": "wrangler dev --config test/production/wrangler.jsonc",
    "deploy:production": "wrangler deploy --config test/production/wrangler.jsonc",
    "types": "cd test/do && wrangler types"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "durable-objects",
    "fetch",
    "queue",
    "cost-optimization",
    "dx"
  ],
  "author": "Lumenize",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "@vitest/coverage-istanbul": "3.2.4",
    "typescript": "^5.9.2",
    "vitest": "3.2.4",
    "wrangler": "^4.38.0"
  },
  "dependencies": {
    "@lumenize/utils": "^0.13.0",
    "ulid-workers": "^2.1.0"
  },
  "peerDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "vitest": "3.2.4"
  }
}
