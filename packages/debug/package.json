{
  "name": "@lumenize/debug",
  "version": "0.19.0",
  "description": "Zero-dependency debug logging for Cloudflare Workers, Node.js, Bun, and browsers",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "vitest --run",
    "coverage": "vitest --run --coverage",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "debug",
    "logging",
    "cloudflare-workers",
    "durable-objects",
    "nodejs",
    "browser"
  ],
  "author": "Larry Maccherone",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
