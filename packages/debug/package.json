{
  "name": "@lumenize/debug",
  "version": "0.17.0",
  "description": "De✨light✨ful scoped debug logging for Cloudflare Durable Objects and browsers",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./client": {
      "import": "./src/client.ts",
      "types": "./src/client.ts"
    }
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "coverage": "COVERAGE=true vitest --run --coverage",
    "types": "wrangler types"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "durable-objects",
    "debug",
    "logging",
    "observability",
    "dx"
  ],
  "author": "Lumenize",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "@vitest/coverage-istanbul": "3.2.4",
    "typescript": "^5.9.2",
    "vitest": "3.2.4",
    "wrangler": "^4.38.0"
  },
  "peerDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "vitest": "3.2.4"
  }
}

