{
  "name": "@lumenize/testing",
  "version": "0.19.0",
  "description": "De✨light✨ful testing utilities for Cloudflare Durable Objects",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "coverage": "COVERAGE=true vitest --run --coverage",
    "types": "wrangler types --config test/wrangler.jsonc"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "durable-objects",
    "DurableObjects",
    "Durable Objects",
    "testing",
    "vitest",
    "dx"
  ],
  "author": "Lumenize",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@cloudflare/actors": "^0.0.1-beta.6",
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "@vitest/coverage-istanbul": "3.2.4",
    "typescript": "^5.9.2",
    "vitest": "3.2.4",
    "wrangler": "^4.38.0"
  },
  "dependencies": {
    "@lumenize/routing": "^0.19.0",
    "@lumenize/rpc": "^0.19.0"
  },
  "peerDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "vitest": "3.2.4"
  },
  "gitHead": "c267b38a6dbc0b8269eb7a96f15fb1ac1e369cba"
}
