{
  "name": "mesh-calls-test",
  "main": "./test-harness.ts",
  "compatibility_date": "2025-09-12",
  "durable_objects": {
    "bindings": [
      {
        "name": "LUMENIZE_CLIENT_GATEWAY",
        "class_name": "LumenizeClientGateway"
      },
      {
        "name": "DOCUMENT_DO",
        "class_name": "DocumentDO"
      },
      {
        "name": "CALCULATOR_DO",
        "class_name": "CalculatorDO"
      },
      {
        "name": "LUMENIZE_AUTH",
        "class_name": "LumenizeAuth"
      }
    ]
  },
  "services": [
    {
      "binding": "SPELLCHECK_WORKER",
      "service": "mesh-calls-test",
      "entrypoint": "SpellCheckWorker"
    },
    {
      "binding": "ANALYTICS_WORKER",
      "service": "mesh-calls-test",
      "entrypoint": "AnalyticsWorker"
    }
  ],
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["LumenizeClientGateway", "DocumentDO"]
    },
    {
      "tag": "v2",
      "new_sqlite_classes": ["LumenizeAuth"]
    },
    {
      "tag": "v3",
      "new_sqlite_classes": ["CalculatorDO"]
    }
  ],
  "ratelimits": [
    {
      "name": "LUMENIZE_AUTH_RATE_LIMITER",
      "namespace_id": "1001",
      "simple": {
        "limit": 100,
        "period": 60
      }
    }
  ],
  "vars": {
    "PRIMARY_JWT_KEY": "BLUE",
    "LUMENIZE_AUTH_REDIRECT": "/app"
  }
}
