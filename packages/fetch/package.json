{
  "name": "@lumenize/fetch",
  "version": "0.19.0",
  "description": "De✨light✨ful fetch utilities for Durable Objects - proxy and direct fetch strategies",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "test": "vitest --run",
    "test:debug": "DEBUG=lmz.* vitest --run",
    "coverage": "vitest --run --coverage",
    "types": "cd test && wrangler types"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "durable-objects",
    "fetch",
    "queue",
    "cost-optimization",
    "dx"
  ],
  "author": "Lumenize",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "@vitest/coverage-istanbul": "3.2.4",
    "typescript": "^5.9.2",
    "vitest": "3.2.4",
    "wrangler": "^4.38.0"
  },
  "dependencies": {
    "@lumenize/debug": "^0.19.0",
    "@lumenize/mesh": "^0.19.0",
    "@lumenize/routing": "^0.19.0",
    "@lumenize/structured-clone": "^0.19.0",
    "@lumenize/test-endpoints": "*"
  },
  "peerDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "vitest": "3.2.4"
  }
}
