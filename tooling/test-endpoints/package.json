{
  "name": "@lumenize/test-endpoints",
  "version": "0.15.0",
  "description": "Test HTTP endpoints for Lumenize integration tests",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "private": true,
  "scripts": {
    "dev": "wrangler dev --show-interactive-dev-session=false",
    "deploy": "wrangler deploy",
    "types": "wrangler types",
    "test": "vitest --run"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "testing"
  ],
  "author": "Lumenize",
  "license": "MIT",
  "files": [
    "src/**/*"
  ],
  "dependencies": {
    "@lumenize/testing": "^0.15.0",
    "@lumenize/utils": "^0.15.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "*",
    "wrangler": "^4.38.0"
  }
}
